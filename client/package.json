{
  "name": "frontend",
  "version": "0.1.0",
  "private": true,
  "browser": {
    "fs": false,
    "path": false,
    "os": false
  },
  "dependencies": {
    "antd": "^5.0.4",
    "axios": "^0.27.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^4.7.1",
    "react-router-dom": "^6.0.1",
    "react-scripts": "^5.0.1"
  },
  "scripts": {
    "start": "concurrently \"react-scripts start\" \"npx tailwind -i src/assets/tailwind.css -o src/tailwind.css -w\"",
    "watch-css": "npx tailwind -i src/assets/tailwind.css -o src/tailwind.css -w",
    "build": "tailwind -i src/assets/tailwind.css -o src/tailwind.css && react-scripts build",
    "lint": "npx prettier --check .",
    "lint:fix": "npx prettier --write .",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  },
  "devDependencies": {
    "concurrently": "^7.6.0",
    "prettier-plugin-tailwindcss": "^0.2.1",
    "tailwindcss": "^3.2.4"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
